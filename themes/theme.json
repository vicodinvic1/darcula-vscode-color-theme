{
	"name": "Darcula Color Theme (vic1)",
	"type": "dark",
	"colors": {
		"editor.background": "#25282f",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#698857"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6897bb"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#9975AB"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#9975AB"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#9975AB",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#CE7827",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#CE7827",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#CE7827",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#ffb64e",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#ffb64e",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#ffb64e",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#9975AB",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#CE7827",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#BABA8A",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#ffb64e",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#ffb64e",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#bd1900",
				"fontStyle": ""
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		},
		{
			"scope": "variable.property, variable.constant, variable.other.property, variable.other.constant, variable.other.object.property, variable.object.property, support.variable.property, meta.definition.property entity.name.function",
			"settings": {
				"foreground": "#9E73AD",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.object.property.js.jsx, meta.object-literal.key",
			"settings": {
				"foreground": "#698857",
				"fontStyle": ""
			}
		},
		{
			"scope": "meta.brace.round.js.jsx, punctuation.definition.string.begin.js.jsx, punctuation.definition.string.end.js.jsx, objectliteral.key.separator, punctuation.separator.key-value, punctuation.definition.template-expression.begin.js.jsx, punctuation.definition.template-expression.end.js.jsx, meta.brace.square.js.jsx, keyword.operator.assignment.js.jsx, punctuation.separator.comma, punctuation.separator.parameter, punctuation.terminator.statement, punctuation.accessor.js.jsx",
			"settings": {
				"foreground": "#A9B7C6"
			}
		},
		{
			"scope": "constant.language.null.js.jsx, constant.language.undefined.js.jsx, constant.language.boolean.true.js.jsx, constant.language.boolean.false.js.jsx",
			"settings": {
				"foreground": "#CE7827"
			}
		}
	]
}
